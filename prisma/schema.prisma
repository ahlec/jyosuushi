generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./server.sqlite"
}

model BugReport {
  browser       String
  clientVersion String
  dateReported  DateTime @default(now())
  id            Int      @default(autoincrement()) @id
  message       String
  os            String
  userAgent     String
}
